
<script>
    var getUserId;
    (function () {
        var key = 'LAYIM_USER_UID';
        function getCookie(name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg))
                return unescape(arr[2]);
            else
                return null;
        }
        getUserId = function () {
            return getCookie(key);;
        }
    })();
</script>